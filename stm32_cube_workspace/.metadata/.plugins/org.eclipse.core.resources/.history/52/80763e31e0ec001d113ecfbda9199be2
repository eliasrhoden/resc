#ifndef SERVO_CTRL
#define SERVO_CTRL


// Park transformation matrices
float park_mat[3][3];
float park_inv_mat[2][3];


typedef struct{
	float rotor_ang;
    float ref_acc, ref_vel, ref_pos;

	int servo_ctrl_cmd;
} ServoCtrl_IN;

typedef struct{
	float ref_Id, ref_Iq;
    float rotor_vel;
} ServoCtrl_OUT;

#endif